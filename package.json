{
  "name": "kvizopkn",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "test": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/coverage/**"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cryptr": "^6.0.1",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-ejs-layouts": "^2.5.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.2",
    "pg": "^7.14.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "supertest": "^4.0.2"
  }
}
